{
  "compilerOptions": {
    "esModuleInterop": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "jsx": "react-jsx",
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "#presentationals/*": ["./src/components/presentationals/*"],
      "#containers/*": ["./src/components/containers/*"],
      "#hooks/*": ["src/hooks/*"],
      "#http/*": ["src/http/*"],
      "#pages/*": ["src/pages/*"],
      "#store/*": ["src/store/*"],
      "#middlewares/*": ["src/middlewares/*"],
      "#utils/*": ["src/utils/*"]
    }
  },
  "include": ["src"]
}
